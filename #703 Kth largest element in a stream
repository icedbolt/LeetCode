class KthLargest:

    def __init__(self, k: int, nums: List[int]):
        # create a min heap with k largest elements
        self.minHeap, self.k = nums, k
        heapq.heapify(self.minHeap)
        while len(self.minHeap) > k:
            heapq.heappop(self.minHeap)

    def add(self, val: int) -> int:
        heapq.heappush(self.minHeap, val)
        if len(self.minHeap) > self.k:
            heapq.heappop(self.minHeap)
        return self.minHeap[0]

/* Create a min heap of all element in nums and pop elements until size of the minHeap becomes k. In this way, we get min heap of k character only, with smallest (Kth largest)
at the root which will be minHeap[0] */
