class Solution:
    def eventualSafeNodes(self, graph: List[List[int]]) -> List[int]:
        # Detect cycle
        n = len(graph)
        safe = {}

        def dfs(i):
            if i in safe:
                return safe[i]
            safe[i] = False
            for nei in graph[i]:
                if not dfs(nei):
                    return False
            safe[i] = True
            return True
        
        ans = []
        for i in range(n):
            if dfs(i):
                ans.append(i)
        return ans
