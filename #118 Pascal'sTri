def generate(self, numRows: int) -> List[List[]]:
    res = [[1]]     # First row
    for i in range(len(numRows) - 1):    # for remaining rows
        temp = [0] + res[-1] + [0]
        row = []    # build next row
        for j in range(len(res[-1]) + 1):    # for next row
            row.append(temp[j] + temp[j+1])     # next row
        res.append(row)
    return res
